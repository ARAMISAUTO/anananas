<?xml version="1.0" encoding="UTF-8"?>
<project name="php">
    <!-- Mandatory targets -->
    <target name="init">
        <dirname file="${ant.file.toolkit}" property="toolkit.module.toolkitdir" />
        <dirname file="${toolkit.module.toolkitdir}/php" property="toolkit.module.basedir" />
        <initConfiguration
            projectDir="${application.dir.base}"
            configurationDir="${toolkit.dir.configuration}"
            moduleDir="${toolkit.module.basedir}"
            moduleName="php" />
    </target>
	<target name="configure" />
	<target name="build" />
	<target name="install" />
	<target name="upload" />
</project>
